<form id="processForm" data-toggle="validator" role="form">
    <div class="form-row">
        <div class="form-group col-md-8">
          <label for="collections">Collections</label>
          <input class="form-control" name="collections" id="collections" value="{{collections}}" required>
        </div>
        <div class="form-group col-md-4">
          <label for="prefix">Prefix</label>
          <input class="form-control" name="prefix" id="prefix" value="{{prefix}}" required>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-3">
          <label for="resolution">Resolution</label>
          <input class="form-control" name="resolution" id="resolution" value="{{resolution}}" required>
        </div>
        <div class="form-group col-md-3">
          <label for="projection">Projection</label>
          <select class="form-control" name="projection" id="projection">
            <option val="latlon" selected>latlon</option>
            <option val="sinusoidal">sinusoidal</option>
          </select>
        </div>
        <div class="form-group col-md-6">
          <label for="bbox">Bounding Box</label>
          <input class="form-control" name="bbox" id="bbox" value="{{bbox}}" required>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
          <label for="timerange_start">Time range start</label>
          <input autocomplete="off" class="form-control datefield" name="timerange_start" id="timerange_start" value="{{timerange_start}}" required>
        </div>
        <div class="form-group col-md-6">
          <label for="timerange_end">Time range end</label>
          <input autocomplete="off" class="form-control datefield" name="timerange_end" id="timerange_end" value="{{timerange_end}}" required>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
          <label for="timeaggre">Time aggregation steps</label>
          <select id="timeaggre" name="timeaggre" class="form-control">
            <option value="noaggre" selected>No selection</option>
            <option value="hourly">Hourly</option>
            <option value="halfdaily">Half daily</option>
            <option value="daily">Daily</option>
            <option value="weekly">Weekly</option>
          </select>
        </div>
    </div>
    <button id="startProcess" style="float:right; margin-right:16px;" class="btn btn-primary">Start Process</button>
</form>