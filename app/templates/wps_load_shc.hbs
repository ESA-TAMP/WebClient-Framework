<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute version="1.0.0" service="WPS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.opengis.net/wps/1.0.0" xmlns:wfs="http://www.opengis.net/wfs" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wcs="http://www.opengis.net/wcs/1.1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd">
  <ows:Identifier>load_shc</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>shc</ows:Identifier>
      <wps:Data>
        <wps:ComplexData><![CDATA[{{shc}}]]></wps:ComplexData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>begin_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{begin_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>end_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{end_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>band</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{band}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>style</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{style}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>dim_range</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{range_min}},{{range_max}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>height</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{height}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    {{#if coefficients_range}}<wps:Input>
      <ows:Identifier>coefficients_range</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{coefficients_range}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput mimeType="image/png" encoding="base64">
      <ows:Identifier>output</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>