<?xml version="1.0" encoding="UTF-8"?>
<wps:Execute version="1.0.0" service="WPS" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.opengis.net/wps/1.0.0" xmlns:wfs="http://www.opengis.net/wfs" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wcs="http://www.opengis.net/wcs/1.1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsAll.xsd">
  <ows:Identifier>calc_diff</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>model_ids</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{model_ids}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>
    {{#if shc}}<wps:Input>
      <ows:Identifier>shc</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{shc}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
    <wps:Input>
      <ows:Identifier>begin_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>2014-09-01T01:01:00Z</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>end_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>2014-09-01T10:11:00Z</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>band</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>F</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>style</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>rainbow</wps:LiteralData>
      </wps:Data>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseDocument lineage="true" storeExecuteResponse="false" status="false">
      <wps:Output mimeType="image/png" encoding="base64">
         <ows:Identifier>difference_image</ows:Identifier>
       </wps:Output>
    </wps:ResponseDocument>
</wps:Execute>